<!DOCTYPE html>

<html>
  <head>
    <title>Simple Markers</title>
    <script src="https://polyfill.io/v3/polyfill.min.js?features=default"></script>
    <!-- jsFiddle will insert css and js -->
    <style>
   
        #map {
         height: 100%;
        }


        html,
        body {
             height: 100%;
             margin: 0;
            padding: 0;
        }
    </style>
  </head>
  <body>
  {% block body %}
    <div id="map"></div>
    <script>
    
    function initMap() {
  const myLatLng = { lat: -25.363, lng: 131.044 };
  const map = new google.maps.Map(document.getElementById("map"), {
    zoom: 4,
    center: myLatLng,
  });

  new google.maps.Marker({
    position: myLatLng,
    map,
    title: "Hello World!",
  });
}
    


    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBkjGiP4QPLWEuAUSpxicLgRk3SrwfNpDU&callback=initMap&v=weekly" defer></script>
    {% endblock %}
  </body>
</html>
